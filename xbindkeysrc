"xvkbd -xsendevent -text '\A\[Left]'"
    F1

"xvkbd -xsendevent -text '\A\[Right]'"
    F2

"xvkbd -xsendevent -text '\Cr'"
    F3

"backlight-brightness --decrease"
    F6

"keyboard-brightness --decrease"
    Alt + F6

"backlight-brightness --increase"
    F7

"keyboard-brightness --increase"
    Alt + F7

"amixer -M set PCM toggle"
    F8

"amixer -M set PCM 10%-"
    F9

"amixer -M set PCM 10%+"
    F10

"xvkbd -xsendevent -text '\[Delete]'"
    Control + BackSpace

"xvkbd -xsendevent -text '\[Home]'"
    Control + Left

"xvkbd -xsendevent -text '\[End]'"
    Control + Right
