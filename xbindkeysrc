"xvkbd -xsendevent -text '\[Delete]'"
    Control + BackSpace

"xvkbd -xsendevent -text '\[Home]'"
    Control + Left

"xvkbd -xsendevent -text '\[End]'"
    Control + Right

"setxkbmap -v us -variant dvorak -option caps:escape"
    m:0x0 + c:248

"amixer set Speaker 3+"
    XF86AudioRaiseVolume

"amixer set Speaker 3-"
    XF86AudioLowerVolume
